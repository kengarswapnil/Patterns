public class Main {
	public static void main(String[] args) {
		int nums [][]={{1,2,3},{4,5,7}};
		
		int count=0;
		 for(int i=0;i<nums.length;i++)
		 {
		 	  for(int j=0;j<nums[0].length;j++)
		 	  {
		 	  	
		 	  if(nums[i][j]==7)
		 	  {
		 	  	  count++;
		 	  }
		 }
		 }
		 System.out.println(count);
		 
	}
}