public class Main {
	
	static void sumrow(int matrix [][])
	{
		    int sum =0;
		   
		    	for(int j=0;j<matrix[0].length;j++){
		    	   sum+=matrix[1][j];
		    }
		    
		    System.out.println("sum of 2d "+sum);
		    
	}
	public static void main(String[] args) {
		int matrix[][]={{1,2,3},{4,5,6,}};
	sumrow(matrix);
}
}